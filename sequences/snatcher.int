[death]
death0 = [shake2]*huff*[waitlong] *huff*[waitlong] Você é uma mocinha tão desrespeitosa...[/shake2]
death1 = [shake2]Me sinto... tão fraco...[waitlong] *huff*[/shake2]
death2 = [shake2]Por favor...[waitlong] *huff*[waitlong] tenha piedade...[/shake2]
death3 = [shake2][waitlong][wait]Por favor... [br] [waitlong][waitlong][br][big]SENTE-SE[/big][/shake2][waitlong][waitlong]
