Chemicals5 = [shake2]Qual é,[br] me dá uma chance![/shake2]
Chemicals6 = [shake2]Estou feliz que[br]tenha reconsiderado![/shake2]
Chemicals7 = [shake2]Você é meio rude,[br]não é?[/shake2]

turned_blue0 = [shake2]...[/shake2]
turned_blue1 = [shake2]Você acabou de me colorir de azul com meu próprio ataque?[/shake2]
turned_blue2 = [shake2]Isso não pode contar, certo? Com certeza não conta?[/shake2]
turned_blue3 = [shake2]Já chega, hora de você [scream][wave]MORRER[wait][/wave][/scream].[/shake2]

Cherry0 = [shake2]Oh não![/shake2]
