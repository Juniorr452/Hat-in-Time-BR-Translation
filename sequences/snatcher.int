death5 = [shake2]Então, vamos fazer um acordo, jovenzinha. Você dá o fora da minha floresta, e nunca mais nos vemos. Parece bom para um acordo?[/shake2]
death6 = [shake2]Não gostou? Que pena! Não é você quem dá as cartas aqui.[/shake2]
death7 = [shake2]Aqui, vou até fazer isso mais fácil pra você![/shake2]
death8 = [shake2]Agora tudo que precisa fazer é assinar![/shake2]

sign0 = [shake2]...Você entende que não é assim como os contratos funcionam, não é?[/shake2]
sign1 = [shake2]Estou te dando uma chance pra cair fora, criança! Não têm pegadinhas ou cláusulas dessa vez! [/shake2]
sign2 = [shake2]E se eu te dar sua alma de volta, vai fazer você sair?[/shake2]
sign3 = [shake2]Sim? Não? Alôôôôô? Você está aí? Por que não está dizendo nada?[/shake2]
sign4 = [shake2].......[/shake2]
sign5 = [shake2]...Você é bem diferente, não é? E uma negociadora difícil.[/shake2]
sign6 = [shake2]Tudo bem, vou melhorar o acordo pra você.[/shake2]
sign7 = [shake2]Aqui, tome minha última [icon:timepiece][timepiece]Peça Temporal[/timepiece]. Se eu continuar vendo você na minha floresta depois disso, vou ficar bem desapontado![/shake2]

contract_soul_return = Você pegou sua alma de volta! Você se sente aliviada, energizada, e só com a quantidade usual de vazio!

[finale]
outside0 = [shake2]Ei criança! Como que está aquela sua alma? Ainda chacoalhando aí dentro?[/shake2]
outside0b = [shake2]Espera. Até que você não conseguiu ela de volta, conseguiu?[/shake2]
outside1 = [shake2]Eu adoraria saber quem é que pensa que é grande o suficiente para [wave]me[/wave] julgar! E daí que eu peguei algumas [small]centenas[/small] de almas - grande coisa![/shake2]
outside2 = [shake2]Faça um favor para todos nós, criança! Vá ver quem é essa que acha que é melhor do que nós! E bata nela![/shake2]
